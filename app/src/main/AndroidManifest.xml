        <activity android:name=".Settings" android:screenOrientation="portrait" android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Statistics" android:screenOrientation="portrait" android:theme="@style/AppTheme.NoActionBar"  />
        <activity android:name=".ExamScore" android:screenOrientation="portrait" android:theme="@style/AppTheme.NoActionBar" />
